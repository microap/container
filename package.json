{
  "name": "@microap/container",
  "version": "1.0.1",
  "description": "微信小程序容器",
  "files": [
    "bin",
    "src",
    "config",
    "project.config.json",
    "global.d.ts",
    "tsconfig.json",
    "babel.config.js"
  ],
  "bin": {"container": "./bin/container"},
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "node bin/container build --type weapp",
    "build:swan": "node bin/container build --type swan",
    "build:alipay": "node bin/container build --type alipay",
    "build:tt": "node bin/container build --type tt",
    "build:h5": "node bin/container build --type h5",
    "build:rn": "node bin/container build --type rn",
    "build:qq": "node bin/container build --type qq",
    "build:jd": "node bin/container build --type jd",
    "build:quickapp": "node bin/container build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.4.7",
    "@tarojs/plugin-framework-react": "3.4.7",
    "@tarojs/react": "3.4.7",
    "@tarojs/runtime": "3.4.7",
    "@tarojs/taro": "3.4.7",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.4.7",
    "@tarojs/webpack-runner": "3.4.7",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-preset-taro": "3.4.7",
    "eslint": "^8.12.0",
    "eslint-config-taro": "3.4.7",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "stylelint": "^14.4.0",
    "typescript": "^4.1.0"
  }
}
